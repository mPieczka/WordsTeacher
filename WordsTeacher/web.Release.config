<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <security>
        <authentication></authentication>
      </security>
      <handlers>
        <remove name="aspNetCore" />
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\WordsTeacher.dll" stdoutLogEnabled="true" stdoutLogFile=".\logs\stdout" xdt:Locator="Match(name)" xdt:Transform="Replace">
        <environmentVariables>
          <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" xdt:Locator="Match(name)" xdt:Transform="Replace" />
        </environmentVariables>
      </aspNetCore>
    </system.webServer>
  </location>
</configuration>